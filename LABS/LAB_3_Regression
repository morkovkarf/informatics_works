def fit(speed, epochs):
	k_list = [k0]
	b_list = [b0]
	for i in range(epochs):
		k = k - speed * get_dk(x, y, k, b)
		b = b - speed * get_db(x, y, k, b)
		k_list.append(k)
		b_list.append(b)
	return k, b

